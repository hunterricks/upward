{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Native",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "declaration": false,
    "declarationMap": false,
    "jsx": "react-native",
    "lib": ["es2019"],
    "module": "commonjs",
    "noEmit": true,
    "resolveJsonModule": true,
    "target": "esnext"
  },
  "include": ["src"],
  "exclude": ["node_modules"]
}
