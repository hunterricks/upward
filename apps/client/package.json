{
  "name": "@upward/client",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "predev": "./scripts/cleanup-ports.sh && ./scripts/start-docker.sh",
    "predev:local": "./scripts/cleanup-ports.sh && ./scripts/start-docker.sh",
    "dev:local": "next dev -p 3000",
    "dev": "dotenv -e .env.local -- concurrently --kill-others-on-fail \"./scripts/cleanup-ports.sh && ./scripts/start-docker.sh && next dev -p 3000\" \"cloudflared tunnel --config tunnel-config.yml run\"",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "clean": "rm -rf .next node_modules",
    "db:migrate": "dotenv -e .env.local -- ./scripts/start-docker.sh",
    "db:studio": "dotenv -e .env.local -- pnpm prisma studio"
  },
  "dependencies": {
    "@auth/core": "^0.37.4",
    "@auth/prisma-adapter": "^2.7.4",
    "@prisma/client": "^6.0.1",
    "@types/bcryptjs": "^2.4.6",
    "bcryptjs": "^2.4.3",
    "dotenv": "^16.0.3",
    "lru-cache": "^11.0.2",
    "next": "14.0.4",
    "next-auth": "^4.24.5",
    "prisma": "^6.0.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.17",
    "autoprefixer": "^10.4.16",
    "concurrently": "^7.6.0",
    "dotenv-cli": "^7.4.4",
    "eslint": "^8.55.0",
    "eslint-config-next": "14.0.4",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.3.6",
    "tailwindcss-animate": "^1.0.7",
    "typescript": "^5.3.3"
  }
}
